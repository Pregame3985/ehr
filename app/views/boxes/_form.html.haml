= form_for @box do |f|
  - if @box.errors.any?
    #error_explanation
      %h2= "#{pluralize(@box.errors.count, "error")} prohibited this box from being saved:"
      %ul
        - @box.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :id
    = f.number_field :id
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :hash
    = f.text_field :hash
  .field
    = f.label :authentication_token
    = f.text_field :authentication_token
  .field
    = f.label :interviewee
    = f.number_field :interviewee
  .actions
    = f.submit 'Save'
